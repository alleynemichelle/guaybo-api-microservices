{
    "$schema": "https://json.schemastore.org/nest-cli",
    "collection": "@nestjs/schematics",
    "sourceRoot": "apps/guaybo-microservices.ts/src",
    "compilerOptions": {
        "deleteOutDir": true,
        "webpack": true,
        "tsConfigPath": "apps/guaybo-microservices.ts/tsconfig.app.json"
    },
    "monorepo": true,
    "root": "apps/guaybo-microservices.ts",
    "projects": {
        "hosts": {
            "type": "application",
            "root": "apps/hosts",
            "entryFile": "main",
            "sourceRoot": "apps/hosts/src",
            "compilerOptions": {
                "tsConfigPath": "apps/hosts/tsconfig.app.json"
            }
        },
